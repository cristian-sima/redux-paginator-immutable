{
  "name": "redux-paginator-immutable",
  "version": "17.0.19",
  "types": "./lib/types/index.d.ts",
  "description": "redux-paginator-immutable helps you deal with paginated API by composing your existing reducers",
  "main": "./lib/index.js",
  "babel": {
    "ignore": [
      "**/__tests__",
      "**/types"
    ],
    "presets": [
      "@babel/env",
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "env": {
      "production": {
        "plugins": [
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-export-default-from",
          "@babel/plugin-transform-modules-commonjs",
          "@babel/plugin-transform-react-constant-elements",
          "@babel/plugin-transform-react-inline-elements",
          "@babel/plugin-transform-typescript",
          "transform-react-remove-prop-types"
        ]
      },
      "developmentTime": {
        "plugins": [
          "transform-decorators-legacy",
          "transform-class-properties"
        ]
      }
    }
  },
  "scripts": {
    "build:lib": "@babel src --extensions .ts,.tsx  --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/redux-paginator-immutable.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/redux-paginator-immutable.min.js",
    "build": "npm run build:lib",
    "clean": "rimraf dist lib",
    "check": "npm-check -u -i babel-jest -i jest --specials=bin,eslint,babel,webpack",
    "eslint": "eslint --ext .jsx,.js,.ts,.tsx src --fix",
    "load": "npm run test && npm run clean && npm run eslint && npm run ts && npm publish",
    "test": "ts-mocha src/__tests__/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive \"src/**/__tests__/*\" --require src/__tests__/setup.js",
    "test:codecov": "cat ./coverage/coverage.json | ./node_modules/.bin/codecov",
    "ts": "tsc --declaration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristian-sima/redux-paginator-immutable.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "redux",
    "paginator",
    "pagination",
    "async",
    "immutablejs",
    "immutable",
    "paginate",
    "paging"
  ],
  "author": "Cristian Sima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cristian-sima/redux-paginator-immutable/issues"
  },
  "homepage": "https://github.com/cristian-sima/redux-paginator-immutable#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.3.4",
    "chai-immutable": "^2.1.0",
    "codecov": "^3.8.1",
    "cross-env": "^6.0.3",
    "es3ify": "^0.2.2",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.23.1",
    "expect": "^24.9.0",
    "flux-standard-action": "^2.1.1",
    "glob": "^7.1.6",
    "isparta": "^4.1.1",
    "mocha": "^6.2.2",
    "npm-check": "^5.9.2",
    "rimraf": "^3.0.2",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0"
  },
  "dependencies": {
    "create-subscription": "^17.0.0",
    "depcheck": "^1.4.3",
    "immutable": "*",
    "query-string": "^7.1.0",
    "react": "^17.0.2",
    "redux": "^4.1.2",
    "reselect": "^4.1.5",
    "superagent": "^7.1.1",
    "x25": "^17.0.17"
  }
}
