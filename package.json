{
  "name": "redux-paginator-immutable",
  "version": "1.0.0",
  "description": "redux-paginator-immutable helps you deal with paginated API by composing your existing reducers",
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/redux-paginator-immutable.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/redux-paginator-immutable.min.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min && node ./prepublish.js",
    "clean": "rimraf dist lib",
    "eslint": "eslint src --fix --ignore-pattern=src/__tests__/**",
    "prepublish": "npm run test && npm run clean && npm run build",
    "test": "mocha --compilers js:babel-register --recursive --recursive \"src/**/__tests__/*\" --require src/__tests__/setup.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive \"src/**/__tests__/*\" --require src/__tests__/setup.js",
    "test:codecov": "cat ./coverage/coverage.json | ./node_modules/.bin/codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristian-sima/redux-paginator-immutable.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "redux",
    "paginator",
    "pagination",
    "async",
    "immutablejs",
    "immutable",
    "paginate",
    "paging"
  ],
  "author": "Cristian Sima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cristian-sima/redux-paginator-immutable/issues"
  },
  "homepage": "https://github.com/cristian-sima/redux-paginator-immutable#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-check-es2015-constants": "^6.22.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-computed-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-es2015-literals": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-object-super": "^6.24.1",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-flow-react-proptypes": "^9.1.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "flow-bin": "^0.59.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.1",
    "es3ify": "^0.2.2",
    "expect": "^21.2.1",
    "flux-standard-action": "^2.0.0",
    "glob": "^7.1.2",
    "isparta": "^4.0.0",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
      "eslint": "^4.11.0"
  },
  "peerDependencies": {
  },
  "dependencies": {
    "redux": "^3.7.2",
    "lodash.pick": "^4.4.0",
    "query-string": "^5.0.1",
    "superagent": "^3.8.1"
  }
}
