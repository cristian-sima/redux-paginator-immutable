{
  "name": "redux-paginator-immutable",
  "version": "17.0.26",
  "types": "./lib/index.d.ts",
  "description": "redux-paginator-immutable helps you deal with paginated API by composing your existing reducers",
  "main": "./lib/index.js",
  "babel": {
    "ignore": [
      "**/__tests__",
      "**/types"
    ],
    "presets": [
      "@babel/env",
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "env": {
      "production": {
        "plugins": [
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-export-default-from",
          "@babel/plugin-transform-modules-commonjs",
          "@babel/plugin-transform-react-constant-elements",
          "@babel/plugin-transform-react-inline-elements",
          "@babel/plugin-transform-typescript",
          "transform-react-remove-prop-types"
        ]
      },
      "developmentTime": {
        "plugins": [
          "transform-decorators-legacy",
          "transform-class-properties"
        ]
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist lib",
    "eslint": "eslint --ext .jsx,.js,.ts,.tsx src --fix",
    "load": "npm run test && npm run clean && npm run eslint && npm run ts && npm publish",
    "test": "ts-mocha src/__tests__/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive \"src/**/__tests__/*\" --require src/__tests__/setup.js",
    "test:codecov": "cat ./coverage/coverage.json | ./node_modules/.bin/codecov",
    "check": "npm run depcheck & npm run check:npm-check",
    "check:npm-check": "npm-check -u -i babel-jest -i jest --specials=bin,eslint,babel,webpack",
    "depcheck": "depcheck --ignores=webpack-cli,@hot-loader/react-dom --ignore-patterns=server,node_modules,tmp,fonts",
    "check:global": "npm-check -gu",
    "ts:check": "tsc --noEmit ",
    "ts": "tsc --declaration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristian-sima/redux-paginator-immutable.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "redux",
    "paginator",
    "pagination",
    "async",
    "immutablejs",
    "immutable",
    "paginate",
    "paging"
  ],
  "author": "Cristian Sima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cristian-sima/redux-paginator-immutable/issues"
  },
  "homepage": "https://github.com/cristian-sima/redux-paginator-immutable#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.3.4",
    "chai-immutable": "^2.1.0",
    "codecov": "^3.8.1",
    "depcheck": "^1.4.3",
    "es3ify": "^0.2.2",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.23.1",
    "expect": "^27.5.1",
    "flux-standard-action": "^2.1.1",
    "glob": "^7.1.6",
    "isparta": "^4.1.1",
    "mocha": "^9.2.0",
    "npm-check": "^5.9.2",
    "rimraf": "^3.0.2",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0"
  },
  "dependencies": {
    "immutable": "*",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "reselect": "^4.1.5",
    "superagent": "^7.1.1",
    "x25": "^17.0.24"
  }
}
